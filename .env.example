# Copy to .env and fill in your values
# DO NOT commit .env to version control

# AWS credentials (use IAM roles in production, not access keys)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1

# Bedrock model IDs
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# Vector store: "faiss" for local dev, "opensearch" for production
VECTOR_STORE=faiss

# OpenSearch (only needed when VECTOR_STORE=opensearch)
OPENSEARCH_URL=https://your-opensearch-endpoint:443
OPENSEARCH_INDEX=fintech-policies
OPENSEARCH_USER=admin
OPENSEARCH_PASSWORD=your_password

# App
LOG_LEVEL=INFO
AGENT_VERBOSE=false
